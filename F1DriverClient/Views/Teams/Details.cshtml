@{
  ViewData["Title"] = $"Details for {Model.TeamName}";
}

<h1>All About @Model.TeamName</h1>

<p>Team Nationality: @Model.TeamNationality</p>
<p>Team Principal @Model.TeamPrincipal</p>

<p>@Html.ActionLink("Edit Details", "Edit", new { id = @Model.TeamId})</p>
<p>@Html.ActionLink("Return to all teams", "Index")</p>